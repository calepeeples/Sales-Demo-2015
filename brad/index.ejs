			<form role="form">
				
				<div class="form-group">
					<p class="h6">Name</p>
					<div class="row">
						<div class="form-group col-sm-6">					
							<input type="text" class="form-control" id="patientFirstName" placeholder="First Name">
						</div>
						<div class="form-group col-sm-6">
