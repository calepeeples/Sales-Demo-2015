<div class="well welcome-home">
	<div class="container">
		<h1>Grand Rounds is now collecting your records</h4>
		<h4>We are now collecting your medical records from each of your medical records providers.</h4>
	</div>
</div>


<div class="container">
  <div class="row">
    <div class="col-lg-12">
    <!-- Page Body Starts -->

		
	<div class="prog">
		<div class="notch" style="left: 50%;"></div>
		<div class="bar">
			<span class="l1">
				<span class="fa fa-check"></span>
				<div class="milestone">Case Started</div>
			</span>
			<span class="l2">
				<span class="fa fa-check"></span>
				<div class="milestone">Collecting Records</div>
			</span>
			<span class="l3">
				<span class="fa circle"></span>
				<div class="milestone">Expert Review</div>
			</span>
			<span class="l4">
				<span class="fa circle"></span>
				<div class="milestone">Expert Opinion</div>
			</span>
			<div class="state" data-n-point="50%" data-percent="50" style="width: 50%;"></div>
		</div>
	</div>

	<div class="service-state-body history_complete"></div>

	<div class="well">
		<div class="row">
			<div class="col-md-1">
				<div class="">[ICON]</div>
			</div>
			
			<div class="col-md-7">
				<div><h4>Choose a Consenting Physician</h4></div>
				<div>&nbsp;</div>
			</div>
			
			<div class="col-md-4">
				<a href="opinion-10" class="btn btn-warning btn-block" data-method="get">start</a>
			</div>
		</div>
	</div>

	
	<div class="row">
	
		<div class="col-md-4">
			<h3>Grand Rounds Team</h3>
			
			<ul>
				
				<li class="col-md-4">
					<div>Image</div>
					<div>
						<h6>You ([name])</h6>
						<small>[you]</small>
					</div>
				</li>

				<li class="col-md-4">
					<div>Image</div>
					<div>
						<h6>[Name]</h6>
						<small>Care Coordinator</small>
					</div>
				</li>

				<li class="col-md-4">
					<div>Image</div>
					<div>
						<h6>[Name]</h6>
						<small>Staff Physician</small>
					</div>
				</li>
				
			</ul>
			
			<div class="form-group">
				<button type="button" class="btn btn-default btn-lg btn-block">Add A Team Member</button>
				<button type="button" class="btn btn-default btn-lg btn-block">View Your Case Documents</button>
			</div>
			
		</div>
	
	
		<div class="col-md-7 col-md-offset-1">
			<h3>Team Messages</h3>
			
			<div class="row">
				<div class="col-md-7">
		  			<form class="group-form">
		  				<textarea name="teammessagesnew" class="form-control" id="teammessagesnew" rows="1" placeholder="New Message..."></textarea>
		  				<div class="attachment">
							<div class="hint">
								<i class="fa fa-paperclip"></i>
								<input name="attachment" type="file">
								<span class="placeholder">Attach a photo or document</span>
							</div>
						</div>
		  			</form>
				</div>
				
				<div class="col-md-1">
		  			<button type="button" class="btn btn-default">Send</button>
				</div>

				<ul>
					<li class="col-md-8">
						<div>[avatar]</div>
						<div>
							<div>
								<p>Message in the message center is here.</p>
							</div>
							<div>
								<div>[names]</div>
								<div>[date]</div>
							</div>
						</div>
					</li>
				</ul>
				
			</div>			
		</div>
	
	</div>
	
	<!-- Page Body Ends -->
    </div>
  </div>
</div>